A,IAAA,CAAA,GAAA,mBAAA,KAAA,MAAA,CAAA,MAAA,CAAA,GAAA,GAAA,IAAA,CAAA,iBAAA;AAAA,UAAA,IAAA,SAAA,IAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,KAHA;AAIA,GA9BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAHA;AAIA,IAAA,kBAJA,gCAIA;AACA,MAAA,OAAA,CAAA,IAAA,CAAA,KAAA,QAAA,CAAA,GAAA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,GAAA,EAAA,KAAA,QAAA,CAAA;AAAA;AAFA,OAAA,EAFA,CAMA;AACA,KAXA;AAYA,IAAA,cAZA,4BAYA;AACA,UAAA,OAAA,CAAA,SAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA,WADA;AAEA,UAAA,MAAA,EAAA;AAAA,YAAA,GAAA,EAAA,KAAA,QAAA,CAAA;AAAA;AAFA,SAAA;AAIA;AACA,KAnBA;AAoBA,IAAA,cApBA,4BAoBA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,GAAA,EAAA,KAAA,QAAA,CAAA;AAAA;AAFA,OAAA;AAIA;AAzBA;AA/BA,CAAA","sourcesContent":["<template>\n  <b-container class=\"bv-example-row mt-3\">\n    <b-row>\n      <b-col>\n        <b-alert show><h3>질문보기</h3></b-alert>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col class=\"text-left\">\n        <b-button variant=\"outline-primary\" @click=\"listQuestion\"\n          >목록</b-button\n        >\n      </b-col>\n      <b-col class=\"text-right\">\n        <b-button\n          variant=\"outline-info\"\n          size=\"sm\"\n          @click=\"moveModifyQuestion\"\n          class=\"mr-2\"\n          >글수정</b-button\n        >\n        <b-button variant=\"outline-danger\" size=\"sm\" @click=\"deleteQuestion\"\n          >글삭제</b-button\n        >\n        <b-button size=\"sm\" @click=\"answerQuestion\">답변하기</b-button>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col>\n        <b-card\n          :header-html=\"`<h3>${question.qno}.\n          ${question.subject} [${question.hit}]</h3><div><h6>${question.userid}</div><div>${question.regtime}</h6></div>`\"\n          class=\"mb-2\"\n          border-variant=\"dark\"\n          no-body\n        >\n          <b-card-body class=\"text-left\">\n            <div v-html=\"message\"></div>\n          </b-card-body>\n        </b-card>\n      </b-col>\n    </b-row>\n    <answer-row\n      v-for=\"(answer, index) in answers\"\n      :key=\"index\"\n      v-bind=\"answer\"\n    />\n  </b-container>\n</template>\n\n<script>\n// import moment from \"moment\";\nimport http from \"@/util/http-common\";\nimport AnswerRow from \"@/components/qna/child/AnswerRow\";\nexport default {\n  data() {\n    return {\n      question: {},\n      answers: [],\n    };\n  },\n  components: {\n    AnswerRow,\n  },\n  computed: {\n    message() {\n      if (this.question.content)\n        return this.question.content.split(\"\\n\").join(\"<br>\");\n      return \"\";\n    },\n    // changeDateFormat() {\n    //   return moment(new Date(this.question.regtime)).format(\n    //     \"YYYY.MM.DD hh:mm:ss\"\n    //   );\n    // },\n  },\n  created() {\n    http.get(`/question/${this.$route.params.qno}`).then(({ data }) => {\n      this.question = data;\n    });\n    http.get(`/answer/${this.$route.params.qno}`).then(({ data }) => {\n      this.answers = data;\n      console.log(this.answers);\n    });\n  },\n  methods: {\n    listQuestion() {\n      this.$router.push({ name: \"QnAList\" });\n    },\n    moveModifyQuestion() {\n      console.warn(this.question.qno);\n      this.$router.replace({\n        name: \"QnAUpdate\",\n        params: { qno: this.question.qno },\n      });\n      //   this.$router.push({ path: `/QnA/modify/${this.question.questionno}` });\n    },\n    deleteQuestion() {\n      if (confirm(\"정말로 삭제?\")) {\n        this.$router.replace({\n          name: \"QnADelete\",\n          params: { qno: this.question.qno },\n        });\n      }\n    },\n    answerQuestion() {\n      this.$router.replace({\n        name: \"QnAAnswer\",\n        params: { qno: this.question.qno },\n      });\n    },\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components/qna"}]}